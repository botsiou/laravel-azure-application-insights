{
  "name": "botsiou/laravel-application-insights",
  "description": "A Laravel 11 package to send application insights to Microsoft Application Insights",
  "version": "1.0.3",
  "keywords": [
    "Yesdevs",
    "insights",
    "analytics",
    "monitoring",
    "laravel"
  ],
  "homepage": "https://github.com/Yesdevs/laravel-application-insights",
  "license": "MIT",
  "authors": [
    {
      "name": "Yanis Kross Yana",
      "email": "yanis.kross@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11|^12",
    "botsiou/application-insights": "dev-master",
    "monolog/monolog": "^3.9"
  },
  "require-dev": {
    "nunomaduro/collision": "^8.7",
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^10"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/botsiou/ApplicationInsights-PHP"
    }
  ],
  "autoload": {
    "psr-4": {
      "Yesdevs\\ApplicationInsights\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yesdevs\\ApplicationInsights\\ServiceProvider"
      ],
      "aliases": {
        "Insights": "Yesdevs\\ApplicationInsights\\Facade"
      }
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yesdevs\\ApplicationInsights\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
